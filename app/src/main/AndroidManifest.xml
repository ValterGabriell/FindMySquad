<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.findmysquad">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FindMySquad">
        <activity
            android:name=".View.AddNewRequisicao"
            android:exported="false" />
        <activity
            android:name=".View.ConfigActivity"
            android:exported="true"></activity>
        <activity
            android:name=".View.LogarActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".View.TelaPrincipalActivity"
            android:exported="false" />
        <activity
            android:name=".View.CadastrarActivity"
            android:exported="true"></activity>
    </application>

</manifest>